{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist",
        ".next"
      ]
    },
    "lint": {
      "dependsOn": [
        "build"
      ]
    },
    "dev": {},
    "deploy": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist"
      ]
    },
    "generate": {
      "dependsOn": [
        "^generate"
      ]
    }
  }
}
